{
  "name": "ambient-link/ability-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A minimal ambient.link ability project recommended to create bare bones artificial intelligence",
  "require": {
    "php": ">=7.0.8",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/flex": "^1.3.1"
  },
  "flex-require": {
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/yaml": "*",
    "ambient-link/ability-sdk": "1.0.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": [
    ],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "3.4.*"
    }
  }
}
